{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "build",
    "types": [],
    "lib": [
      "ES6",
      "DOM"
    ],
    "skipLibCheck": true,
    "noEmit": false,
    "resolveJsonModule": true,
  },
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "node_modules",
  ]
}
